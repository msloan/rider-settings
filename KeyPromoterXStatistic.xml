<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Attach to Process...">
        <Item>
          <IdeaActionID>XDebugger.AttachToProcess</IdeaActionID>
          <ShortCut>'Ctrl+Alt+F5'</ShortCut>
          <Description>Attach to Process...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Database">
        <Item>
          <IdeaActionID>ActivateDatabaseToolWindow</IdeaActionID>
          <ShortCut>'Ctrl+Alt+3'</ShortCut>
          <Description>Database</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'Alt+5'</ShortCut>
          <Description>Debug</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'Attach to Unity Editor &amp; Play'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'Attach to Unity Editor &amp; Play'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug...">
        <Item>
          <IdeaActionID>ChooseDebugConfiguration</IdeaActionID>
          <ShortCut>'Alt+Shift+F9'</ShortCut>
          <Description>Debug...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Errors In Solution">
        <Item>
          <IdeaActionID>ActivateErrorsInSolutionToolWindow</IdeaActionID>
          <ShortCut>'Ctrl+Alt+2'</ShortCut>
          <Description>Errors In Solution</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Search Everywhere">
        <Item>
          <IdeaActionID>SearchEverywhere</IdeaActionID>
          <ShortCut>'Ctrl+T'</ShortCut>
          <Description>Search Everywhere</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop...">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Vim Emulator">
        <Item>
          <IdeaActionID>VimPluginToggle</IdeaActionID>
          <ShortCut>'Ctrl+Alt+V'</ShortCut>
          <Description>Vim Emulator</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>